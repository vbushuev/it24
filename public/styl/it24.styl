// out: ../css/it24.css, compress: true
.heading
    padding: .2em .6em;
    vertical-align: middle;
    font-weight: 600;
.clearfix
    clear:both;
    height:1rem;
    display: block;
    width: 100%;
.item
    line-height: 4em;
    //height:4em;
    cursor: pointer;
    transition: all 0.4s ease-out;
    font-size: 90%;
    position: relative;
    .multirows
        line-height: 1em;
        height: 4em;
        margin: 0;
        display: inline-block;
        padding:.4em .2em;
    div > .error-message
        display: none;
        position:absolute;
        top:1em;
        left:3em;
        transition: all .4s ease-out
        padding 1em;
        width 42rem;
        line-height: 1em;
        background-color: #fff;
        color:red;
        border: dotted 1px red;
        z-index: 999;
    div.failed:hover > .error-message
        display: block;
.item:hover
    background-color: rgba(0,0,0,.1);
.item.status-failed
    background-color: rgba(255,0,0,.2);
.item.status-failed:hover
    background-color: rgba(255,0,0,.1);
.modal
    .title
        font-size: 1.4em;
        font-weight: 400;
    .supplier-title
        font-size: 110%;
        font-weight: 600;
    .input-group
        margin-bottom: 1rem;
.input-group-addon
    min-width: 14rem;
.modal-header h5
    display inline-block
.dropdown-submenu
    position: relative;
.dropdown-submenu>.dropdown-menu
    top: 0;
    left: 100%;
    margin-top: -6px;
    margin-left: -1px;
    -webkit-border-radius: 0 6px 6px 6px;
    -moz-border-radius: 0 6px 6px;
    border-radius: 0 6px 6px 6px;
.dropdown-submenu:hover>.dropdown-menu
    display: block;
.dropdown-submenu>a:after
    display: block;
    content: " ";
    float: right;
    width: 0;
    height: 0;
    border-color: transparent;
    border-style: solid;
    border-width: 5px 0 5px 5px;
    border-left-color: #ccc;
    margin-top: 5px;
    margin-right: -10px;
.dropdown-submenu:hover>a:after
    border-left-color: #fff;
.dropdown-submenu.pull-left
    float: none;
.dropdown-submenu.pull-left>.dropdown-menu
    left: -100%;
    margin-left: 10px;
    border-radius: 6px 0 6px 6px;
.filter-check
    position: absolute;
    left:1em;
.submenu
    margin-left: 1em;
